{
    "manifest_version": 2,
    "name": "MyNetWatch",
    "description": "View online usage data and statistics",
    "version": "1.0",
    "icons": { 
        "16": "/images/icon16.png", 
        "48": "/images/icon48.png", 
        "128": "/images/icon128.png" 
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "idle",
        "chrome://favicon/*",
        "webNavigation",
        "unlimitedStorage"
    ],
    "offline_enabled": true,
    "background": {
        "service_worker": "background.js"
    },
    "browser_action": {
        "default-title": "Click to view usage",
        "default_popup": "index.html",
        "default_title": "MyNetWatch",
        "default_icon": {
            "16": "/images/icon16.png", 
            "48": "/images/icon48.png", 
            "128": "/images/icon128.png" 
        }
    }
}